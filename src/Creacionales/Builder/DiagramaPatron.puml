@startuml

class Client

class Director{
- builder: Builder
+ Director(builder)
+ changeBuilder(builder)
+ make(type)
}

interface Builder{
+ reset()
+ buildStepA()
+ buildStepB()
+ buildStepZ()
}

class ConcreteBuilder1{
- result: Product1
+ buildStepA()
+ buildStepB()
+ buildStepZ()
+ getResult(): Product1
}

class ConcreteBuilder2{
- result: Product2
+ buildStepA()
+ buildStepB()
+ buildStepZ()
+ getResult(): Product2
}

class Product1

class Product2

Builder <|.. ConcreteBuilder1
Builder <|.. ConcreteBuilder2
Product1 <-- ConcreteBuilder1
Product2 <-- ConcreteBuilder2
Builder <-- Director
Director <-- Client
ConcreteBuilder1 <.. Client
@enduml